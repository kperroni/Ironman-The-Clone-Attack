<?xml version="1.0"?>
 
<configuration>
    <system.webServer>
    <modules runAllManagedModulesForAllRequests="true" >
    </modules>
    <staticContent>
      <remove fileExtension=".mp4" />
      <mimeMap fileExtension=".mp4" mimeType="video/mp4" />
      <remove fileExtension=".json" />
      <mimeMap fileExtension=".json" mimeType="application/json" />
    </staticContent>
  </system.webServer>
</configuration> 